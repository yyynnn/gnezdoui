  "module": "build-es/index.js",
  "files": [
    "build",
    "build-lib",
    "build-es",
    "src",
    "types"
  ],
  "types": "./types/index.d.ts",
  "scripts": {
    "dev:wds": "cross-env NODE_ENV=development IS_WDS=1 webpack-dev-server --config ./webpack/wds.babel.js",
    "dev:ssr": "cross-env NODE_ENV=development IS_SSR=1 NODE_ICU_DATA=node_modules/full-icu babel-node --inspect --extensions '.js,.jsx' server/index.js",
    "build:prepare": "npm run build && rm -rf ./docs/ && npm run docz:build",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel ./src --out-dir build-lib",
    "build:es": "cross-env BABEL_ENV=es babel ./src --out-dir build-es",
    "build:umd": "cross-env NODE_ENV=production IS_SSR=1 NODE_ICU_DATA=node_modules/full-icu webpack --mode production --config ./webpack/prod.babel.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run escheck",
    "escheck": "npm run escheck:commonjs && npm run escheck:es && npm run escheck:umd",
    "escheck:commonjs": "es-check es5 build-lib/*/**.js",
    "escheck:es": "es-check --module es6 build-es/*/**.js",
    "escheck:umd": "es-check es5 build/**.js",
    "test": "npm run test:unit",
    "test:unit": "jest --silent --verbose --config ./jest/unit.config.js",
    "test:build_bundle": "rm -rf build/* && npm run build && npm run test:bundle",
    "test:bundle": "jest --config ./jest/dist.config.js",
    "test:coverage": "jest --coverage",
    "test:clear": "jest --clearCache",
    "test:u": "npm test -- -u",
    "lint": "eslint ./src/",
    "clean": "rm -rf build/*",
    "todos": "leasot --exit-nicely ./src/**/*.js ./src/**/*.jsx",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "commit": "npx git-cz",
    "commit:retry": "npx git-cz --retry",
    "prepublishOnly": "npm run build:prepare",
    "semantic-release:dry": "semantic-release --dry-run",
    "semantic-release": "semantic-release"
  },
